{
  "manifest_version": 3,
  "name": "Read Later",
  "version": "1.0",
  "description": "Save pages to read later.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  }
}
